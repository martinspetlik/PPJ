    <target name="compile.module.ppj" depends="compile.module.ppj.production,compile.module.ppj.tests"
            description="Compile module PPJ"/>

    <target name="compile.module.ppj.production" depends="register.custom.compilers"
            description="Compile module PPJ; production classes">
        <mkdir dir="${ppj.output.dir}"/>
        <javac2 destdir="${ppj.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}"
                memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.ppj}/javac">
            <compilerarg line="${compiler.args.ppj}"/>
            <bootclasspath refid="ppj.module.bootclasspath"/>
            <classpath refid="ppj.module.production.classpath"/>
            <src refid="ppj.module.sourcepath"/>
            <patternset refid="excluded.from.compilation.ppj"/>
        </javac2>

        <copy todir="${ppj.output.dir}">
            <fileset dir="${module.ppj.basedir}/src/main/java">
                <patternset refid="compiler.resources"/>
                <type type="file"/>
            </fileset>
            <fileset dir="${module.ppj.basedir}/src/main/resources">
                <patternset refid="compiler.resources"/>
                <type type="file"/>
            </fileset>
        </copy>
    </target>

    <target name="compile.module.ppj.tests" depends="register.custom.compilers,compile.module.ppj.production"
            description="compile module PPJ; test classes" unless="skip.tests">
        <mkdir dir="${ppj.testoutput.dir}"/>
        <javac2 destdir="${ppj.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}"
                memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.ppj}/javac">
            <compilerarg line="${compiler.args.ppj}"/>
            <bootclasspath refid="ppj.module.bootclasspath"/>
            <classpath refid="ppj.module.classpath"/>
            <src refid="ppj.module.test.sourcepath"/>
            <patternset refid="excluded.from.compilation.ppj"/>
        </javac2>

        <copy todir="${ppj.testoutput.dir}">
            <fileset dir="${module.ppj.basedir}/src/test/java">
                <patternset refid="compiler.resources"/>
                <type type="file"/>
            </fileset>
        </copy>
    </target>

    <target name="clean.module.ppj" description="cleanup module">
        <delete dir="${ppj.output.dir}"/>
        <delete dir="${ppj.testoutput.dir}"/>
    </target>
